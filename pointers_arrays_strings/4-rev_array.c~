#include "main.h"
#include <stdio.h>

/**
 * rev_string - check the code.
 *
 *@s: charctere
 * Return: Always 0.
 */
void reverse_array(int *a, int n)
{
int frontIndex = 0;
int backIndex;

backIndex = n - 1;
while (frontIndex < backIndex)
{
char temp = a[frontIndex];
a[frontIndex] = a[backIndex];
a[backIndex] = temp;
frontIndex++;
backIndex--;
}
}
