#include <stdio.h>
#include "main.h"

/**
 * times_table - Computes the absolute value of an integer.
 *
 * Return: The absolute value of the integer.
 */
void times_table(void)
{
  int t, n, r, u, d;
  for(t = 0; t <= 9; t++)
    {
      for(n=0; n <= 9; n++)
	{
	  r = n * t;
	  if (n > 9)
			{
			u = n % 10;
			d = (n - u) / 10;
			_putchar(44);
			_putchar(32);
			_putchar(d + '0');
			_putchar(u + '0');
	}
	  _putchar(r);
	}
      _putchar('\n');
    }
}
